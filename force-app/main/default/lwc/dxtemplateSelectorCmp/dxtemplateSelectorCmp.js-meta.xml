<?xml version="1.0"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
	<apiVersion>59.0</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>DXTemplates PDF Generator</masterLabel>
	<targets>
		<target>lightning__AppPage</target>
		<target>lightning__HomePage</target>
		<target>lightning__RecordPage</target>
		<target>lightning__RecordAction</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightning__RecordPage">
			<supportedFormFactors>
				<supportedFormFactor type="Small" />
				<supportedFormFactor type="Large" />
			</supportedFormFactors>
		    <property label="Template Name" name='defaultTemplateName' type="String" />
			<property label="Page Size" name='defaultPageSize' type="String" />
			<property label="Page Orientation" name='defaultPageOrientation' type="String" />
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>